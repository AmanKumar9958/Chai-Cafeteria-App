{
  "expo": {
    "name": "ChaiCafeteriaApp",
    "slug": "ChaiCafeteriaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chai-cafeteria-icon.png",
    "scheme": "chaicafeteriaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amandev.chaicafeteriaapp"
    },
    "android": {
      "package": "com.amandev.chaicafeteriaapp",
      "adaptiveIcon": {
        "backgroundColor": "#FDEADB",
        "foregroundImage": "./assets/images/chai-cafeteria-foreground-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/chai-cafeteria-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FDEADB",
          "dark": {
            "backgroundColor": "#FDEADB"
          }
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "de99c04c-37b0-42a8-be26-c3ac8751db91"
      }
    }
  }
}
